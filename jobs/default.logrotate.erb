<%= "#{@config.prefix}/var/log/jobs/#{@job_name}" %>.*.log {
        su <%= @config.user %> <%= @config.group %>
        weekly
        missingok
        rotate 15
        copytruncate
        compress
        delaycompress
        notifempty
        create 0664 <%= @config.user %> <%= @config.group %>
        sharedscripts
        postrotate
          #
        endscript
}